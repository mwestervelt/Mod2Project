<h1>Add New Resolution: </h1>
<%= form_for @resolution do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :description %><br>
  <%= f.text_area :description %> <br>
  <%= f.label :end_date %>
  <%= f.date_select :end_date %> <br>
  <%= f.label "Done?" %>
  <%= f.check_box :done %> <br>
  <%= f.label :tag %>
  <%= f.collection_select :tag_ids, @tags, :id, :name %> <br>
  <%= f.submit %>
  <% end %>
